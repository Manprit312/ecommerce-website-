{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
   "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  
  },
  "files": [
  "src/types/model-viewer.d.ts" // Add the specific file path here
],
  "include": [
  "next-env.d.ts",
  "src/types/**/*.d.ts",
  "src/types/**/*.d.ts",
  "**/*.ts",
  "**/*.tsx",
   "**/*.tsx",
  ".next/types/**/*.ts"
], "exclude": [
    "node_modules"
  ]
}
